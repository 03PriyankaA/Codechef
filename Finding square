#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int res = (int)sqrt(n);
        cout<<res<<endl;
    }
    return 0;
}
